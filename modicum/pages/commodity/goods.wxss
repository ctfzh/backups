  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: bold;
}
.settlement_an .mo_fu{
  font-size: 26rpx;
}
.settlement_sele{
  color: #fff;
  background-color: #008866;
}
.sku_mask{
  position: fixed;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
   background-color: #fff; 
  z-index: 11;
  transform:translateY(100%);
  transition:all 0.15s ease;
}

.sku_goods{
  position: relative;
  display: flex;
  margin: 30rpx 30rpx 0rpx 30rpx;
  padding-bottom: 10rpx;
  box-shadow: 0rpx 20rpx 30rpx #FFFFFF;
  z-index: 1;
}
.sku_goods_img{
  width: 160rpx;
  height: 160rpx;
