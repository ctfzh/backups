/* 遮罩层背景 */
.goods_mask{
  position: fixed;
  top: 0rpx;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  background-color: #000;
  opacity: 0.5;
  z-index: 10;
}
.goods_top{
  position: relative;
  height: 85rpx;
  background-color: #008866;
  color: #fff;
  overflow: hidden;
}
.goods_top_img{
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  z-index: 0;
}
.goods_top_img image{
  width: 100%;
  height: 100%;
}
.goods_top_content{
   position: relative;
  height: 100%;
  display: flex;
  align-items: center;
  margin: 0rpx 40rpx;
  z-index: 1; 
  font-size:14px;
  overflow: hidden;
}
.goods_title{
  font-size: 36rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  margin-bottom: 10rpx;
  font-weight: bold;
}
.goods_title_text{
  flex: 1;
  margin-right: 20rpx;
}
.goods_search{
  position: relative;
  width:30rpx;
  height: 30rpx;
  display: flex;
}
.goods_search image{
  width: 100%;
  height: 100%;
}
.goods_search_zone{
  position: absolute;
  top: -20rpx;
  bottom: -20rpx;
  left: -20rpx;
  right: -20rpx;
}
.evaluate{
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.star{
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
  display: flex;
  position: relative;
}
.star image{
  width: 20rpx;
  height: 20rpx;
}
.bottom_img{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  bottom: 0rpx;
  font-size: 0rpx;
  overflow: hidden;
}
.sales_volume{
  margin-left: 10rpx;
  font-size: 24rpx;
}
.notice{
  width: 642rpx;
  font-size: 28rpx;
  -webkit-line-clamp: 2; /** 显示的行数 **/
}
.goods_middle{
  position: absolute;
  left: 0rpx;
  right: 0rpx;
  top: 0rpx;
  bottom: 100rpx;
  display: flex;
  flex-flow: row nowrap;
}

.goods_middle_left{
  width: 170rpx;
  height: 100%;
  background-color: #F0F1F2;
}
.goods_middle_menu{
  width: 100%;
  height: 100%;
}
.menu_item{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  padding: 34rpx 30rpx;
  word-break:break-all;
  border-bottom: 1rpx solid #E8E9EB;
}
.menu_item_sele{
  background-color: #fff;
  font-weight: bold;
}
.goods_middle_right{
  position: relative;
  flex: 1;
  height: 100%;
}
.goods_list_item:last-child{
  min-height: 100%;
  padding-bottom: 5rpx;
}
.top_goods_type{
  position: relative;
  z-index: 1;
}
.goods_type{
  height: 100rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  padding: 0rpx 20rpx;
  background-color: #fff;
}
.goods_list{
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  z-index: 0;
}


.goods_item{
	position: relative;
  height: 150rpx;
  display: flex;
  flex-flow: row nowrap;
  margin-bottom: 30rpx;
  align-items: center;
}
.goods_item:last-child{
  margin-bottom: 0rpx;
}
.goods_img{
  width: 146rpx;
  height: 146rpx;
  display: flex;
  align-items: center;
  margin: 0rpx 20rpx; 
  overflow: hidden;
}
.goods_img image{
  width: 100%;
  height: 100%;
}
.goods_inof{
	width: 319rpx;
   height: 100%;
   display: flex;
   flex-flow: column;
   justify-content: space-between;
}
.goods_name{
  flex: 1;
  margin-right: 20rpx;
  /* padding-top: 10rpx; */
  font-size: 30rpx;
  font-weight: bold;
  -webkit-line-clamp: 2; /** 显示的行数 **/
}

.goods_money{
  /* padding-bottom: 10rpx; */
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: bold;
}
.mo_fu{
  font-size: 30rpx;
}
.goods_item .purchase{
	position: absolute;
	bottom: -20rpx;
	right: -18rpx;
}
.purchase{
  height: 100%;
  display: flex;
  align-items: flex-end;
  padding-right: 30rpx;
}
.purchase_choice{
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 25rpx;
  margin: 15rpx 0rpx ;
}
.purchase_join{
  display: flex;
  flex-flow: row nowrap;
  /* margin-bottom: 10rpx; */
}
.join{
   width: 80rpx;
   height: 80rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}

.join image{
  width: 44rpx;
  height: 44rpx;
}
.num{
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0rpx 15rpx;
  margin: 0rpx -15rpx;
}

.goods_bottom{
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  height: 100rpx;
  background-color: #3C3E42;
  color: #909499;
  font-size: 24rpx;
  display: flex;
  z-index: 11;
}
.goods_shopping_cart{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #65676B;
  margin: -20rpx 0rpx 0rpx 30rpx;
  border-radius: 100%;
