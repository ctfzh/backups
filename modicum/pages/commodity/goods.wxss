}
.goods_shopping_cart image{
  width: 60rpx;
  height: 60rpx;
}
.red_dot{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #8B4513;
  color: #fff;
  border-radius: 100%;
  top: 0rpx;
  right: -20rpx;
}
.settlement_inof{
  flex: 1;
  height: 100%;
  display: flex;
  flex-flow: column;
  justify-content: center;
  margin-left: 20rpx;
}
.settlement_money{
  color: #fff;
  font-size: 44rpx;
  line-height: 40rpx;
}
.settlement_money .mo_fu{
  font-size: 36rpx;
}
.distribution_money{
  font-size: 24rpx;
  margin-top: 5rpx;
}
.settlement_an{
  width: 220rpx;
  height: 100%;
  background-color: #2C2F33;
