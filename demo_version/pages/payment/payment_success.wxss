/* 支付成功CSS */
page{
  background-color: #fff;
}
image{
  width: 100%;
  height: 100%;
}
.payment_img{
  width: 120rpx;
  height: 120rpx;
  margin: 60rpx auto 0rpx auto;
}
.payment_text{
  text-align: center;
  font-size: 32rpx;
  color: #09BB07;
  margin-top: 30rpx;
}
.payment_money{
  text-align: center;
   margin-top: 80rpx; 
  font-size: 80rpx;
  font-weight: bold;
   line-height: 80rpx; 
   padding-bottom: 25rpx; 
}
.mo_fu{
  font-size: 48rpx;
}
.payment_detailed{
  margin: 0rpx 50rpx;
  border-width: 0rpx;
  border-style: solid;
  border-top-width: 1rpx;
  border-bottom-width: 1rpx;
}
.detailed_list{
  padding-top: 25rpx;
  font-size: 28rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.detailed_list:last-child{
  padding-bottom: 25rpx; 
}

.complete_an{
  width: 330rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #09BB07;
  border-radius: 7rpx;
  color: #09BB07;
  text-align: center;
  margin: 242rpx auto 0rpx auto;
  font-size: 28rpx;
}