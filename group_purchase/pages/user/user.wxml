			<view class='jump_img'><image src='../img/income/order.png'></image></view>
			<view class='jump_text'>我的订单</view>
		</view>
		<view class='arrow_img'><image src='../img/income/arrow.png'></image></view>
	</view>
	
	<view class='jump_item' data-url='/pages/collection/collection' catchtap='jump'>
		<view class='jump_title'>
			<view class='jump_img'><image src='../img/income/collection.png'></image></view>
			<view class='jump_text'>我的收藏</view>
		</view>
		<view class='arrow_img'><image src='../img/income/arrow.png'></image></view>
	</view>
	
	<view class='jump_item'>
		<view class='jump_title'>
			<view class='jump_img'><image src='../img/income/service.png'></image></view>
			<view class='jump_text'>联系客服</view>
		</view>
		<view class='arrow_img'><image src='../img/income/arrow.png'></image></view>
	</view>

</view>